# PostgreSQL development parameters
export HOME_POSTGRES_SRC=$HOME/git/postgres
export HOME_POSTGRES_MAKE=$HOME/make/postgres
export HOME_POSTGRES_INSTALL=$HOME/bin/pgsql

# Add Postgres binaries to path
export PATH=$HOME_POSTGRES_INSTALL/bin:$PATH

